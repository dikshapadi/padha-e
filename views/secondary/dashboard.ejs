                                        const apiKey = 'TIx4QZvalAhFEQdRVBGQhYZxbLkbbIis';
                                        const year = new Date().getFullYear(); // Get the current year
                                        const response = await fetch(`https://calendarific.com/api/v2/holidays?&api_key=${apiKey}&country=IN&year=${year}`);
                                        const data = await response.json();
                        
                                        // Process the received data and add holidays to the HTML
                                        const holidaysList = document.getElementById('holidays-list');
                                        data.response.holidays.forEach(holiday => {
